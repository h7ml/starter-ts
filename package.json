{
  "name": "dext7r-starter-ts",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.9.0",
  "description": "this is a test package for dext7r",
  "author": "dext7r",
  "license": "MIT",
  "homepage": "https://github.com/dext7r/starter-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dext7r/starter-ts.git"
  },
  "bugs": "https://github.com/dext7r/starter-ts/issues",
  "keywords": [
    "dext7r",
    "starter",
    "ts",
    "typescript",
    "starter"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "dext7r-starter-ts": "./dist/index.cjs"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.1",
    "@types/node": "^22.10.2",
    "bumpp": "^9.9.2",
    "eslint": "^9.17.0",
    "esno": "^4.8.0",
    "nr": "^0.1.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2",
    "unbuild": "^3.0.1",
    "vite": "^6.0.5",
    "vitest": "^2.1.8"
  }
}
